{
  "name": "server",
  "version": "1.0.0",
  "description": "Authentication server setup for Emaily application",
  "main": "index.js",
  "engines": {
    "node": "8.4.0",
    "npm": "5.3.0"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "lint": "eslint .",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild":
      "NPM_CONFIG_PRODUCTION=false yarn install --prefix client && yarn build --prefix client"
  },
  "author": "Joseph Morse <tamed.lionheart@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.2",
    "concurrently": "^3.5.0",
    "cookie-session": "^1.3.0",
    "express": "^4.15.4",
    "mongoose": "^4.11.7",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "stripe": "^4.24.1"
  },
  "devDependencies": {
    "eslint": "^4.4.1",
    "nodemon": "^1.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jnmorse/udemy-emaily-server.git"
  },
  "keywords": ["react", "udemy", "nodejs", "full-stack"],
  "bugs": {
    "url": "https://github.com/jnmorse/udemy-emaily-server/issues"
  },
  "homepage": "https://github.com/jnmorse/udemy-emaily-server#readme"
}
